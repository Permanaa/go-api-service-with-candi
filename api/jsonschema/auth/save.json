{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "JSON Schema for save",
	"type": "object",
	"properties": {
		"email": {
			"type": "string",
			"format": "email",
			"minLength": 1,
			"errorMessage": "error email message"
		},
		"password": {
			"type": "string",
			"minLength": 1
		}
	},
	"required": [ "email", "password" ],
	"additionalProperties": false
}
